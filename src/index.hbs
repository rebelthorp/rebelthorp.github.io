    <div class="home__section">
      <h2 class="main__title">
        What we do
      </h2>
      <p class="main__desc">
        {{#each @root.data.skills}}
          {{#if @index}}
            &nbsp;â€¢&nbsp;
          {{/if}}
          <span class="main__desc-item">
            {{this}}
          </span>
        {{/each}}
      </p>
    </div>
    <div class="home__section">
      <h2 class="main__title">
        Who we are
      </h2>
      <p class="main__desc">
        {{@root.data.about}}
      </p>
    </div>
  </section>
  {{> contact}}
</main>
{{> footer}}
{{> analytics}}
</body>
</html>
